<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.one">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.One"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".act.MarqueeviewAct"
            android:exported="false" />
        <activity
            android:name=".act.RollingTextAct"
            android:exported="false" />
        <activity
            android:name=".act.MyTimerAct"
            android:exported="false" />
        <activity
            android:name=".act.CalculatorAct"
            android:exported="false" />
        <activity
            android:name=".act.CodeAct"
            android:exported="false" />
        <activity
            android:name=".act.AnswerAct"
            android:exported="true" />
        <activity
            android:name=".act.QuestionAct"
            android:exported="true" />
        <activity
            android:name=".act.ThreadAct"
            android:exported="false" />
        <activity
            android:name=".act.AlarmAct"
            android:exported="false" />
        <activity
            android:name=".act.MainAct"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name=".server.AlarmReceiver"
            android:process=":remote" />
    </application>

</manifest>